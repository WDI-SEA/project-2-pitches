<div class="container">
    <h1 class="text-center">My Favorite Markets</h1>
    <p class="text-center">
      <a href= '/'>Home Page</a>
    </p>
  
    <% markets.forEach(function(market) { %>
      <div class="faves">
        <h2><%= market.name %></h2>
        <form method="GET" action="market/<%= market.name %>">
            <!-- <input hidden type="text" name="id" value="<%= pokemon.id %>"> -->
            <input hidden type="text" name="name" value="<%=market.name%>">
            <!-- <button class="btn btn-primary" type="submit"></button> -->
          </form>
      </div>
    <% }); %>
  </div>